{  "include": ["packages/*/src"],  "compilerOptions": {    "target": "ES2020",    "module": "ESNext",    "moduleResolution": "Bundler",    "strict": true,    "noUnusedLocals": true,    "noUnusedParameters": true,    "noFallthroughCasesInSwitch": true,    "allowJs": false,    "skipLibCheck": true,    "resolveJsonModule": true,    "esModuleInterop": true,    "allowSyntheticDefaultImports": true,    "forceConsistentCasingInFileNames": true,    "useDefineForClassFields": true,    "sourceMap": true,    "isolatedModules": true,    "baseUrl": "./packages",    "lib": ["ESNext", "DOM", "DOM.Iterable"],    "paths": {      "@vue/*": ["packages/*/src"]    }  },  "exclude": ["node_modules"]}